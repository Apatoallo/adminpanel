  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "delete-maps": "yarn run delete-map-files && yarn run delete-references-to-map-files",
    "delete-map-files": "find ./build -name '*.map' -delete",
    "delete-references-to-map-files": "find ./build -regex '.*\\.\\(js\\|css\\)' -exec sed -i -E '\\/[\\*\\/]#\\ssourceMappingURL=main(\\.[0-9a-f]+)?\\.(css|js)\\.map(\\*\\/)?/g' {} +",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "local-start": "REACT_APP_ENV_NAME=LOCAL yarn start",
    "dev-start": "REACT_APP_ENV_NAME=DEV yarn start",
    "prod-start": "REACT_APP_ENV_NAME=PROD yarn start",
    "test-start": "REACT_APP_ENV_NAME=TEST yarn start",
    "dev-build": "REACT_APP_ENV_NAME=DEV yarn build",
    "test-build": "REACT_APP_ENV_NAME=TEST yarn build",
    "prod-build": "REACT_APP_ENV_NAME=PROD yarn build && yarn run delete-maps"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
